        function findFib(){
            inputNum = parseInt(document.getElementById("inputWordFib").value);
            fib =[0, 1, 1];
            for(let b = 0; b <= inputNum; b++){
                fib.push(fib[b+1] + fib[b+2]);
            }
            document.getElementById("answer3").innerHTML = fib[inputNum - 1];

        }
